@model Vidly.Models.Customer
@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<li>@Html.DisplayNameFor(m => m.MembershipType): @Model.MembershipType.Name</li>
@if (@Model.Birthdate != null)
{
    <li>Birthdate: @Html.DisplayFor(c => c.Birthdate)</li>
}
